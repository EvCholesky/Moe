	parse "(decl paN (ptr ([] $count $type)))"
	typecheck "(&[$count]$type paN (&[$count]$type ([$count]$type Literal:Int## $type)))"
	{
		$type(s32|s8|f32|bool) + $count(1|2|1000)
	}

