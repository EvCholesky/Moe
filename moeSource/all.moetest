		{
			$atype(4) + $patype("4 ") + $tcatype("Literal:Int64 "),
			$atype("") + $patype("") + $tcatype(""),
			$atype(..$errid(2011)) + $patype("") + $tcatype(""),
		},
		$ret("->int") + $type(int)
		{
			$atype(4) + $patype("4 ") + $tcatype("Literal:Int64 "),
			$atype("") + $patype("") + $tcatype(""),
